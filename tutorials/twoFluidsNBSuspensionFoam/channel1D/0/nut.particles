FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut.particles;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 2 -1 0 0 0 0];


internalField       uniform 0;


boundaryField
{
    "walls"
    {
        type               calculated;
        value              $internalField;
    }

    "outlet|inlet"
    {
        type               zeroGradient;
        value              $internalField;
    }

    "cyc.*"
    {
        type cyclic;
    }

}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

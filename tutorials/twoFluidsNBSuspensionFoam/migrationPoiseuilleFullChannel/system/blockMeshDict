FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
convertToMeters 1;

Hx  1.7712;
Hy  0.0018;
Hz  1e-05;

vertices
(

    name v0 (0 0 0)
    name v1 ( $Hx 0 0)
    name v2 ( $Hx  $Hy 0)
    name v3 (0  $Hy 0)

    name v4 (0 0 $Hz)
    name v5 ( $Hx 0 $Hz)
    name v6 ( $Hx  $Hy $Hz)
    name v7 (0  $Hy $Hz)

);

blocks
(
  hex (v0 v1 v2 v3 v4 v5 v6 v7) (100 20 1 )  simpleGrading (50 1 1)
);

edges
(
);

patches
(

    patch inlet
    (
        (v0 v3 v7 v4)
    )
    patch outlet
    (
        (v1 v5 v6 v2)
    )
    empty symmetry0
    (
        (v0 v1 v2 v3)
    )
    empty symmetry1
    (
        (v4 v7 v6 v5)
    )
    wall walls
    (
        (v0 v1 v5 v4)
        (v3 v2 v6 v7)
    )
);

